@page
@model Project334.Pages.BusinessActivities.CreateModelNew

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>BusinessActivity</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="BusinessActivity.BusinessID" class="control-label"></label>
                <input asp-for="BusinessActivity.BusinessID" value="@ViewContext.HttpContext.Request.Query["id"]" class="form-control" readonly="readonly" />
                <span asp-validation-for="BusinessActivity.BusinessID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BusinessActivity.WorkingDate" class="control-label"></label>
                <input asp-for="BusinessActivity.WorkingDate" class="form-control" />
                <span asp-validation-for="BusinessActivity.WorkingDate" class="text-danger"></span>
                @Html.ValidationMessageFor(m => m.BusinessActivity)
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
